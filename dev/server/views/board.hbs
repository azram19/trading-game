<div id="canvasWrapper">
  <canvas width="{{x}}" height="{{y}}" id="water" keepalive="true"></canvas>
  <canvas width="{{x}}" height="{{y}}" id="background" keepalive="true"></canvas>
  <canvas width="{{x}}" height="{{y}}" id="animations" keepalive="true"></canvas>
